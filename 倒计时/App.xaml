<Application
    x:Class="倒计时.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:倒计时"
    xmlns:media="using:Windows.UI.Xaml.Media"
    xmlns:viewModels="using:倒计时.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
            <viewModels:ViewModelLocator x:Name="Locator" />
            <Color x:Key="SystemAccentColor">#F9FAFE</Color>
            <media:AcrylicBrush x:Key="CustomAcrylicBackgroundBrush" BackgroundSource="HostBackdrop"
        TintOpacity="0.6" TintColor="CornflowerBlue" FallbackColor="CornflowerBlue"/>
            <AcrylicBrush x:Key="CustomAcrylicInAppBrush" BackgroundSource="HostBackdrop"
        TintOpacity="0.9" TintColor="White" FallbackColor="White" />
            <media:AcrylicBrush x:Key="CustomAcrylicBackgroundBrush2" BackgroundSource="HostBackdrop"
        TintOpacity="0.7" TintColor="AliceBlue" FallbackColor="PowderBlue" />
            <SolidColorBrush x:Key="GlobalTheme" x:FieldModifier="public"/>
            <FontFamily x:Key="CalculatorFontFamily">ms-appx:///Assets/CalcMDL2.ttf#Calculator MDL2 Assets</FontFamily>
            <AcrylicBrush x:Key="ModuleBackground" BackgroundSource="HostBackdrop" TintColor="{ThemeResource SystemChromeMediumColor}" TintOpacity="0.5"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
